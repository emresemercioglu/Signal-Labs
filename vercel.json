{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/saas.html",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/contact.html",
      "destination": "/contact",
      "permanent": true
    },
    {
      "source": "/about.html",
      "destination": "/about",
      "permanent": true
    },
    {
      "source": "/services.html",
      "destination": "/services",
      "permanent": true
    },
    {
      "source": "/blog.html",
      "destination": "/blog",
      "permanent": true
    },
    {
      "source": "/faq.html",
      "destination": "/faq",
      "permanent": true
    },
    {
      "source": "/the-ci-treadmill-is-broken-why-battlecards-age-in-days-not-quarters.html",
      "destination": "/blog/the-ci-treadmill-is-broken-why-battlecards-age-in-days-not-quarters",
      "permanent": true
    },
    {
      "source": "/the-ci-treadmill-is-broken-why-battlecards-age-in-days-not-quarters",
      "destination": "/blog/the-ci-treadmill-is-broken-why-battlecards-age-in-days-not-quarters",
      "permanent": true
    },
    {
      "source": "/competing-in-real-time-a-ci-playbook-for-high-growth-teams.html",
      "destination": "/blog/competing-in-real-time-a-ci-playbook-for-high-growth-teams",
      "permanent": true
    },
    {
      "source": "/competing-in-real-time-a-ci-playbook-for-high-growth-teams",
      "destination": "/blog/competing-in-real-time-a-ci-playbook-for-high-growth-teams",
      "permanent": true
    },
    {
      "source": "/www.usesignallabs.com/:path*",
      "destination": "https://usesignallabs.com/:path*",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=()"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(?:jpg|jpeg|png|gif|webp|svg|ico))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(?:js|css))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

